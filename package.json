    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "bcrypt": "^5.1.0",
    "crypto-js": "^4.1.1",
    "csv-parser": "^3.0.0",
    "dataloader": "^2.2.2",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^9.0.0",
